<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backround10"
    tools:context="gr.aueb.softeng.view.Owner.Statistics.StatisticsActivity">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="719dp"
        android:layout_height="537dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128"
        app:srcCompat="@drawable/gradient7" />

    <TextView
        android:id="@+id/MonthlyIncomeText"
        android:layout_width="230dp"
        android:layout_height="27dp"
        android:layout_marginTop="44dp"
        android:text="@string/monthly_income"
        android:textAlignment="center"
        android:textColor="#040303"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/MonthlyIncomeResult"
        android:layout_width="252dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/YearlyIncomeText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.358"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MonthlyIncomeText"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/YearlyIncomeText"
        android:layout_width="195dp"
        android:layout_height="26dp"
        android:text="@string/yearly_income"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/YearlyIncomeResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.403"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/YearlyIncomeResult"
        android:layout_width="251dp"
        android:layout_height="33dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:id="@+id/AverageSalesForCustomerText"
        android:layout_width="362dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="@string/average_expenses_for_each_customer"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.673"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/YearlyIncomeResult" />

    <TextView
        android:id="@+id/AverageExpensesForCustomerResult"
        android:layout_width="261dp"
        android:layout_height="39dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.306"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AverageSalesForCustomerText" />

    <TextView
        android:id="@+id/AverageDailyRevenueText"
        android:layout_width="269dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:text="@string/verage_daily_revenue"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.394"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AverageExpensesForCustomerResult" />

    <TextView
        android:id="@+id/AverageDailyRevenueResult"
        android:layout_width="194dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.364"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AverageDailyRevenueText" />

    <TextView
        android:id="@+id/OrderCancellationRateText"
        android:layout_width="306dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:text="@string/order_cancellation_rate"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.292"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AverageDailyRevenueResult" />

    <TextView
        android:id="@+id/OrderCancellationRateResult"
        android:layout_width="229dp"
        android:layout_height="39dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#070707"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.313"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/OrderCancellationRateText"
        app:layout_constraintVertical_bias="0.055" />

    <Button
        android:id="@+id/button"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:text="Button"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="622dp" />


</androidx.constraintlayout.widget.ConstraintLayout>